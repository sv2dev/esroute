{
  "name": "esroute",
  "version": "0.3.0",
  "description": "A small efficient framework-agnostic client-side routing library, written in TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc & npm run esbuild",
    "esbuild": "esbuild src/index.ts --bundle --sourcemap --format=esm --outfile=dist/index.js",
    "esbuild:demo": "esbuild demo/demo.ts --bundle --sourcemap --minify --watch --format=iife --outfile=demo/demo.js",
    "test": "jest",
    "start": "node demo/server.js & npm run esbuild:demo",
    "prepublish": "tsc"
  },
  "keywords": [
    "route",
    "routing"
  ],
  "repository": "github:svi3c/esroute",
  "author": "Sven Rogge <sreglitzki@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.164",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "esbuild": "^0.14.34",
    "jest": "^27.5.1",
    "jest-mock": "^27.5.1",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.6.3"
  }
}
